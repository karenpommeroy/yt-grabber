{
  "name": "yt-grabber",
  "version": "1.9.2",
  "description": "Youtube Grabber - robust desktop application designed to retrieve multimedia from YouTube and YouTube Music services",
  "keywords": [
    "youtube",
    "youtube music",
    "grabber",
    "downloader",
    "music",
    "albums",
    "artists",
    "album releases",
    "songs",
    "discography",
    "yt-dlp",
    "youtube playlist",
    "youtube channel",
    "youtube video",
    "youtube audio",
    "youtube mp3",
    "youtube wav",
    "youtube mp4",
    "youtube mpeg",
    "youtube mkv",
    "youtube avi",
    "youtube gif"
  ],
  "main": "./dist/index.js",
  "repository": {
    "url": "git+https://github.com/karenpommeroy/yt-grabber.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "concurrently \"cross-env NODE_ENV=development yarn webpack-tsx --watch\" \"yarn run electron\"",
    "start:debug": "concurrently \"cross-env NODE_ENV=development yarn webpack-tsx --watch\" \"yarn run electron\"",
    "webpack-tsx": "tsx node_modules/webpack/bin/webpack.js --config webpack.config.ts",
    "build": "yarn clean && \"cross-env NODE_ENV=development yarn webpack-tsx\"",
    "build:prod": "yarn clean && \"cross-env NODE_ENV=production yarn webpack-tsx\"",
    "electron": "wait-on dist/index.js && ELECTRON_DISABLE_SECURITY_WARNINGS=true electron .",
    "electron-debug": "wait-on dist/index.js && ELECTRON_DISABLE_SECURITY_WARNINGS=true electron --inspect-brk=9229 .",
    "test": "jest",
    "pack": "yarn build:prod && electron-builder --dir",
    "dist": "yarn build:prod && electron-builder",
    "e2e:codegen": "playwright test e2e/codegen.spec.ts --debug --workers=1",
    "e2e:prod": "cross-env CI=e2e playwright test",
    "e2e:dev": "cross-env CI=e2e E2E_DEV=true playwright test"
  },
  "build": {
    "appId": "yt.grabber",
    "productName": "YT Grabber",
    "copyright": "© 2025 Marcin Karpiński",
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "publish": null,
    "asar": true,
    "win": {
      "target": "nsis",
      "icon": "dist/resources/icons/logo.ico",
      "artifactName": "${name}-${version}-setup.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "installerIcon": "dist/resources/icons/logo.ico",
      "uninstallerIcon": "dist/resources/icons/logo.ico",
      "installerHeaderIcon": "dist/resources/icons/logo.ico",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "YT Grabber"
    },
    "directories": {
      "output": "release"
    },
    "extraResources": [
      {
        "from": "dist/resources/bin",
        "to": "bin",
        "filter": [
          "*.exe"
        ]
      },
      {
        "from": "dist/resources/locales",
        "to": "locales"
      },
      {
        "from": "public/profile",
        "to": "profile"
      }
    ]
  },
  "author": "Marcin Karpiński <mkarpins@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.4",
    "@mui/lab": "^7.0.1-beta.18",
    "@mui/material": "^7.3.4",
    "axios": "^1.12.2",
    "classnames": "^2.5.1",
    "electron-devtools-installer": "^4.0.0",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-store": "^8.2.0",
    "fs-extra": "^11.3.2",
    "i18next": "^25.6.0",
    "i18next-node-fs-backend": "^2.1.3",
    "jsonschema": "^1.5.0",
    "lodash-es": "^4.17.21",
    "mkdirp": "^3.0.1",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "music-metadata": "^11.10.3",
    "puppeteer": "^24.26.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-devtools": "^2.4.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.1.5",
    "react-number-format": "^5.4.4",
    "react-router-dom": "^7.9.4",
    "usehooks-ts": "^3.1.1",
    "win-version-info": "^6.0.1",
    "winston": "^3.18.3",
    "yt-dlp-wrap": "^2.3.12"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/i18next": "^13.0.0",
    "@types/i18next-node-fs-backend": "^2.1.5",
    "@types/jest": "^30.0.0",
    "@types/jsonschema": "^1.1.1",
    "@types/lodash-es": "^4.17.12",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/moment-duration-format": "^2.2.6",
    "@types/mui-image": "^1.0.5",
    "@types/node": "^24.9.1",
    "@types/prettier": "^3.0.0",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-env": "^1.18.8",
    "@types/win-version-info": "^3.1.3",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.1.0",
    "css-loader": "^7.1.2",
    "electron": "^38.4.0",
    "electron-builder": "^26.0.12",
    "electron-playwright-helpers": "^1.8.2",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-css": "^0.11.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-webpack-plugin": "^5.0.2",
    "globals": "^16.4.0",
    "html-webpack-plugin": "^5.6.4",
    "i18next-scanner-webpack": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-html-reporter": "^4.3.0",
    "jiti": "^2.6.1",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.6.2",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.0.1",
    "source-map-loader": "^5.0.0",
    "stylus": "^0.64.0",
    "stylus-loader": "^8.1.2",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "util": "^0.12.5",
    "wait-on": "^9.0.1",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "yarn@4.4.1"
}
