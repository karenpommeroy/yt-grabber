{
  "name": "yt-grabber",
  "version": "0.0.1",
  "description": "Youtube Grabber",
  "main": "./dist/index.js",
  "repository": {
    "url": "https://github.com/karenpommeroy/yt-grabber.git"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "concurrently \"cross-env NODE_ENV=development yarn run watch\" \"yarn run electron\"",
    "build": "cross-env NODE_ENV=production && yarn clean && webpack --config webpack.config.ts",
    "electron": "wait-on dist/index.js && cross-env NODE_ENV=development electron .",
    "watch": "webpack --watch --config webpack.config.ts",
    "pack": "yarn run build && electron-builder --dir",
    "dist": "yarn run build && electron-builder"
  },
  "build": {
    "appId": "yt.grabber",
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release"
    }
  },
  "author": "Marcin Karpi≈Ñski <mkarpins@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.2",
    "classnames": "^2.5.1",
    "electron-devtools-installer": "^4.0.0",
    "electron-store": "^8.2.0",
    "fs-extra": "^11.3.0",
    "i18next": "^24.2.2",
    "i18next-node-fs-backend": "^2.1.3",
    "jsonschema": "^1.5.0",
    "lodash": "^4.17.21",
    "mkdirp": "^3.0.1",
    "moment": "^2.30.1",
    "puppeteer": "^24.1.1",
    "puppeteer-core": "^24.1.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-devtools": "^2.4.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "react-number-format": "^5.4.3",
    "react-router-dom": "^7.1.5",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/fs-extra": "^11.0.4",
    "@types/i18next": "^13.0.0",
    "@types/i18next-node-fs-backend": "^2.1.5",
    "@types/jsonschema": "^1.1.1",
    "@types/lodash": "^4.17.15",
    "@types/node": "^22.13.1",
    "@types/prettier": "^3.0.0",
    "@types/puppeteer-core": "^7.0.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/webpack-dev-server": "^4.7.2",
    "@types/webpack-env": "^1.18.8",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-css": "^0.11.0",
    "eslint-plugin-react": "^7.37.4",
