    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-node-externals": "^3.0.0"
  },
  "packageManager": "yarn@4.4.1",
  "dependenciesMeta": {
    "electron@34.0.2": {
      "unplugged": true
    }
  }
}
